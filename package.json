{
  "name": "my-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "docker-compose -f docker-compose.dev.yaml up",
    "dev-build": "docker-compose -f docker-compose.dev.yaml up --build",
    "prod": "docker-compose up -d",
    "prod-build": "docker-compose up --build -d",
    "down-docker": "docker-compose down && docker-compose -f docker-compose.dev.yaml down"
  }
}
